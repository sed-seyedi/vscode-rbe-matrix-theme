{
	"name": "RBE Matrix Skin Theme",
	"type": "dark",
	"tokenColors": [
		{
			"settings": {
				"background": "#000000",
				"foreground": "#00FF00"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#888888"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#AAAAFF"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF3333"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#AAAAFF"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#CCFFCC"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#34bcec"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#0033FF"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF8888"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF3333"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF6666"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "",
				"foreground": "#1D8B2D"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#44FF44"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#66FF66"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": " bold",
				"foreground": "#66FF66"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#7dd9a4"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"foreground": "#AAAAFF"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": " bold underline",
				"foreground": "#d93a3a"
			}
		},
		{
			"name": "Support Class",
			"scope": "support.class",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#AD7FA8"
			}
		}
	],
